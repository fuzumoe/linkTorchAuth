{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NestJS (start)",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "APP_PORT": "3000",
        "DB_TYPE": "postgres",
        "DB_HOST": "localhost",
        "DB_PORT": "5432",
        "DB_USERNAME": "linkTorch_username",
        "DB_PASSWORD": "linkTorch_password",
        "DB_DATABASE": "linktorch_auth",
        "JWT_SECRET": "your_jwt_secret_key",
        "JWT_EXPIRES_IN": "1d"
      }
    },
